<script>
    $(function(){
        $(document).on('click','.tambah-waktu',function(e){
            e.preventDefault();
            $("#myModalDetail-project").modal('show');
            $.post("https://www.fprtm.com/nosotros/project/tambah_waktu",
                {id:$(this).attr('data-id')},
                function(html){
                    $(".content-body").html(html);
                }   
            );
        });
    });
