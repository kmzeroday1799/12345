<script>
   $(document).ready(function () {
        size_li = $("#myList .col-md-3").size();
        x=4;
        $('#myList .col-md-3:lt('+x+')').show();
        $('#loadMore').click(function () {
            x= (x+24 <= size_li) ? x+24 : size_li;
            $('#myList .col-md-3:lt('+x+')').show();
            $('#showLess').show();
            if(x == size_li){
                $('#loadMore').hide();
            }
        });
        $('#showLess').click(function () {
            x=(x-24<0) ? 4 : x-24;
            $('#myList .col-md-3').not(':lt('+x+')').hide();
            $('#loadMore').show();
            $('#showLess').show();
            if(x == 4){
                $('#showLess').hide();
            }
        });
    });
  </script>
